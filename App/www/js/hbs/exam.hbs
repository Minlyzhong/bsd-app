{{#.}}
<li class="swipeout" data-id="{{id}}" data-name="{{subject}}" data-count="{{count}}" data-time="{{totalTime}}" data-point="{{totalScore}}" data-userName="{{userName}}" data-subjectTypeName="{{subjectTypeName}}">
	<div class="swipeout-content item-content">
		<div class="item-media" style="border-radius: 5px;padding: 5px;background-color: 
		{{#examList_bgColor index}}{{/examList_bgColor}}
		;">
		</div>
		<div class="item-inner">
			<div class="item-title" style="font-size: 15px;">{{subject}}</div>
			<div class="item-after" style="font-size: 14px;">{{count}}道</div>
		</div>
	</div>
	<div class="swipeout-actions-right">
		<a href="#" class="swipeout-close swipeAction bg-orange">简介</a>
	</div>
</li>
{{/.}}