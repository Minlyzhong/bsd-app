{{#.}}
<li class="item-content" data-id = "{{id}}" data-loadTypeId = "{{logTypeId}}" data-workType = "{{logTypeId}}">
	<div class="item-media">
		<div><img style="margin-left: 20px;margin-right: 20px;width: 50px; height: 50px; border-radius: 50%;border: 1px solid #a7745c;" src="{{logPic}}" /></div>
	</div>
	<div class="item-inner">
		<div class="item-title">{{logTitle}}</div>
		<div>
			<div class="item-after" style="right: 40px;font-size: 15px;color: black;margin-top: 20px;position: absolute;">--{{name}}</div>
			<div class="item-after" style="float: right;font-size: 15px;color: black;margin-top: 40px;color:gray;margin-right: 20px;">{{modifyTime}}</div>
		</div>
	</div>
</li>
{{/.}}