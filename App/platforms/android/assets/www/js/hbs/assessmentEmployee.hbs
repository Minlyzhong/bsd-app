{{#.}}
	<li>
		<a href="#" class="item-link item-content assessPeopleListContent" data-id="{{id}}" data-name="{{name}}" data-count="{{count}}">
			<div class="item-media media-avater">
				<img src="img/avater.png" />
			</div>
			<div class="item-inner">
				<div class="item-title-row">
					<div class="item-title k-group-t">{{name}}</div>
					{{#compare point '>' 0}}
					<div class="item-after k-group-after" style="color: green !important;">总分: {{point}}分</div>
					{{else}}
					<div class="item-after k-group-after">总分: {{point}}分</div>
					{{/compare}}
				</div>
				<div class="item-subtitle k-group-sub">
					<span class="">更新时间: {{date}}</span>
					<span class="s-r">考核总项: {{count}}项</span>
				</div>
			</div>
		</a>
	</li>
{{/.}}