{{#.}}
<li>
	<a href="#" class="item-link item-content signDetailContent" data-id="{{id}}" data-name="{{name}}">
		<div class="item-media media-avater">
			<img src="img/avater.png" />
		</div>
		<div class="item-inner">
			<div class="item-title-row">
				<div class="item-title">{{name}}</div>
			</div>
			<div class="item-subtitle signSubtitle" style="padding-top: 5px;">
				<span class="">{{title}}：{{count}}次</span>
			</div>
		</div>
	</a>
</li>
{{/.}}